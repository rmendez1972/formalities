<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>scr_reports</title>
        <link rel="stylesheet" href="css/ui-lightness/jquery-ui-1.10.4.custom.min.css">
        
        <!-- framework jquery -->
        <script type="text/javascript"  src="js/jquery-1.7.2.js"></script>
        <script type="text/javascript" src="js/jquery-ui-1.10.4.custom.min.js"></script>
        
        <script>
            $(function() {
                $( "#tabs" ).tabs();
                
                $(":button").click(function(event) 
                {  
                   
                   event.preventDefault();  
                    var url = $(this).attr('value');
                    
                    
                    $.post(url,  function(resultado) 
                    {  
                        $('#contenido').html(resultado);  
                    });  
                });
            });
                        
        </script>
    </head>
    <body>
        
        <div id="reports">
            <h1>Reportes de sistema</h1>
        </div>
        <br/>
        <br/>
        <br/>
        <br/>
        
        <div id="tabs">
            <ul>
              <li><a href="#rep-operacionales">Reportes Operacionales</a></li>
              <li><a href="#rep-administrativos">Reportes Administrativos</a></li>

            </ul>
            <div id="rep-operacionales">
                <table class="reportes">
                    <tbody>
                        <tr>
                            <td width="30%">
                                <button value="controladorreportes?operacion=frm_reportesolicitudes" id="rep_solicitudes" >
                                        <img src="imagenes/editar.png" class="btn-report" alt="Primer Reporte" title="Primer Reporte"/>
                                        <p>Reporte Solicitudes</p>
                                </button>
                            </td>
                            <td width="30%">
                                <button value="controladorregistro?operacion=localizar&id_solicitud=${solicitudes.id_solicitud}" id="editar" 
                                        titulo="Confirme la edición." mensaje="Está Ud. seguro de editar esta solicitud!">
                                        <img src="imagenes/editar.png" class="btn-report" alt="Primer Reporte" title="Primer Reporte"/>
                                        <p>Segundo Reporte</p>
                                </button>
                            </td>
                            <td width="30%">
                                <button value="controladorregistro?operacion=localizar&id_solicitud=${solicitudes.id_solicitud}" id="editar" 
                                        titulo="Confirme la edición." mensaje="Está Ud. seguro de editar esta solicitud!">
                                        <img src="imagenes/editar.png" class="btn-report" alt="Primer Reporte" title="Primer Reporte"/>
                                        <p>Tercer Reporte</p>
                                </button>
                            </td>
                        </tr>
                        <tr>
                            <td width="30%">
                                <button value="controladorregistro?operacion=localizar&id_solicitud=${solicitudes.id_solicitud}" id="editar" 
                                        titulo="Confirme la edición." mensaje="Está Ud. seguro de editar esta solicitud!">
                                        <img src="imagenes/editar.png" class="btn-report" alt="Primer Reporte" title="Primer Reporte"/>
                                        <p>Cuarto Reporte</p>
                                </button>
                            </td>
                            <td width="30%">
                                <button value="controladorregistro?operacion=localizar&id_solicitud=${solicitudes.id_solicitud}" id="editar" 
                                        titulo="Confirme la edición." mensaje="Está Ud. seguro de editar esta solicitud!">
                                        <img src="imagenes/editar.png" class="btn-report" alt="Primer Reporte" title="Primer Reporte"/>
                                        <p>Quinto Reporte</p>
                                </button>
                            </td>
                            <td width="30%">
                                <button value="controladorregistro?operacion=localizar&id_solicitud=${solicitudes.id_solicitud}" id="editar" 
                                        titulo="Confirme la edición." mensaje="Está Ud. seguro de editar esta solicitud!">
                                        <img src="imagenes/editar.png" class="btn-report" alt="Primer Reporte" title="Primer Reporte"/>
                                        <p>Sexto Reporte</p>
                                </button>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div id="rep-administrativos">
              <table class="reportes">
                    <tbody>
                        <tr>
                            <td width="30%">
                                <button value="controladorreportes?operacion=frm_reporteusuarios" id="rep_usuarios" >
                                        <img src="imagenes/myspace alt2122.png" class="btn-report" alt="usuarios" title="Usuarios Sistema"/>
                                        <p>Usuarios de sistema</p>
                                </button>
                            </td>
                            <td width="30%">
                                <button value="controladorregistro?operacion=localizar&id_solicitud=${solicitudes.id_solicitud}" id="editar" 
                                        titulo="Confirme la edición." mensaje="Está Ud. seguro de editar esta solicitud!">
                                        <img src="imagenes/listar.png" class="btn-report" alt="Tramites" title="Trámites"/>
                                        <p>Trámites de la SEDUVI</p>
                                </button>
                            </td>
                            <td width="30%">
                                <button value="controladorregistro?operacion=localizar&id_solicitud=${solicitudes.id_solicitud}" id="editar" 
                                        titulo="Confirme la edición." mensaje="Está Ud. seguro de editar esta solicitud!">
                                        <img src="imagenes/requisitos.png" class="btn-report" alt="requisitos" title="Requisitos"/>
                                        <p>Requisitos por Trámite</p>
                                </button>
                            </td>
                        </tr>
                        
                    </tbody>
                </table>
            </div>
            
        </div>
        
    </body>
</html>
